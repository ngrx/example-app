{
  "name": "angular2-webpack-lite",
  "version": "1.0.0",
  "description": "Angular2 Webpack Lite Boilerplate",
  "main": "index.ts",
  "scripts": {
    "material:backup": "cpy '**/*.css' '../@material2-backup' --cwd=node_modules/@angular2-material --parents",
    "material:clean": "rimraf \"./node_modules/@angular2-material/**/*.css\"",
    "material:restore": "cpy '**/*.css' '../@angular2-material' --cwd=node_modules/@material2-backup --parents",
    "build:aot": "ngc -p tsconfig.prod.json",
    "prebuild": "npm run build:aot && npm run material:backup && npm run material:clean",
    "build": "webpack -p --env.prod",
    "postbuild": "npm run material:restore",
    "server:dev": "webpack-dashboard -- webpack-dev-server --hot",
    "start": "npm run server:dev",
    "test": "karma start --single-run",
    "test:watch": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheLarkInn/angular2-webpack-lite.git"
  },
  "author": "Sean Larkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheLarkInn/angular2-webpack-lite/issues"
  },
  "homepage": "https://github.com/TheLarkInn/angular2-webpack-lite#readme",
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.7",
    "@angular/compiler": "2.0.0-rc.7",
    "@angular/compiler-cli": "^0.6.0",
    "@angular/core": "2.0.0-rc.7",
    "@angular/forms": "2.0.0-rc.7",
    "@angular/http": "2.0.0-rc.7",
    "@angular/platform-browser": "2.0.0-rc.7",
    "@angular/platform-browser-dynamic": "2.0.0-rc.7",
    "@angular/platform-server": "^2.0.0-rc.7",
    "@angular/router": "^3.0.0-rc.2",
    "@angular2-material/button": "2.0.0-alpha.8-1",
    "@angular2-material/card": "2.0.0-alpha.8-1",
    "@angular2-material/core": "2.0.0-alpha.8-1",
    "@angular2-material/icon": "2.0.0-alpha.8-1",
    "@angular2-material/input": "2.0.0-alpha.8-1",
    "@angular2-material/list": "2.0.0-alpha.8-1",
    "@angular2-material/progress-circle": "^2.0.0-alpha.8-1",
